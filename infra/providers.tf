terraform {
  required_version = ">= 1.6.0"

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.80"
    }
  }
}

provider "aws" {

  region  = var.aws_region
  
  profile = var.aws_profile

  default_tags {
    tags = {
      Project     = "wash"
      ManagedBy   = "terraform"
      Environment = "prod"
    }
  }
}